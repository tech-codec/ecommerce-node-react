{
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps",
      "NODE_VERSION": "18"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
